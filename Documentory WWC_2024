#include <iostream>
using namespace std;

int main() {
    // Declare variables to store the academic biodata
    string name = "Vanshika";
    string UID = "23BCS80009";
    char section = 'B';
    string area_of_interest = "Computer Science Engineering";
    int age = 18;
    double last_year_CGPA = 7.45;
    string DOB = "2006-03-07";  // Date of Birth in YYYY-MM-DD format
    string hometown = "Himachal Pradesh";

    // Printing the academic biodata
    cout << "Academic Biodata:" << endl;
    cout << "Name: " << name << endl;
    cout << "UID: " << UID << endl;
    cout << "Section: " << section << endl;
    cout << "Area of Interest: " << area_of_interest << endl;
    cout << "Age: " << age << endl;
    cout << "Last Year's CGPA: " << last_year_CGPA << endl;
    cout << "Date of Birth: " << DOB << endl;
    cout << "Hometown: " << hometown << endl;

    return 0;
}
